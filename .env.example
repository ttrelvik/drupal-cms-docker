# Copy this file to .env and fill in real secret values. DO NOT commit .env.
# Database connection used by Drupal (and/or by the Postgres container)
DB_HOST=db
DB_NAME=drupal
DB_USER=drupal
DB_PASSWORD=change_me

# Postgres container bootstrap variables (optional if using DB_* vars)
POSTGRES_DB=drupal
POSTGRES_USER=drupal
POSTGRES_PASSWORD=change_me
